{
  "name": "ip2d",
  "version": "1.0.7",
  "main": "index.js",
  "author": "0xflotus",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xflotus/ip2d.git"
  },
  "scripts": {
    "prepare": "tsc index.ts",
    "prepublishOnly": "yarn run test",
    "pretest": "yarn run lint",
    "test": "jest test.ts",
    "posttest": "yarn run size",
    "lint": "tslint index.ts",
    "size": "bundlesize --files index.js --max-size 0.5kb --compression none",
    "postsize": "yarn run sizeGzip",
    "sizeGzip": "bundlesize --files index.js --max-size 0.25kb --compression gzip",
    "postsizeGzip": "yarn run sizeBrotli",
    "sizeBrotli": "bundlesize --files index.js --max-size 0.2kb --compression brotli",
    "postsizeBrotli": "yarn run limit",
    "limit": "size-limit"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "axios": "^0.19.0",
    "bundlesize": "^0.18.0",
    "jest": "^24.9.0",
    "size-limit": "^2.1.6",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "yarn": "^1.19.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 0
      }
    },
    "displayName": {
      "name": "ip2d-test",
      "color": "grey"
    }
  }
}
